import geometry as gmtry;
import stats as stts;

var radius = 5;
var area = gmtry.calculateArea(radius);
var circumference = gmtry.calculateCircumference(radius);
var numbers = "1,2,3,4,5";
var mean = stts.calculateMean(numbers);
var variance = stts.calculateVariance(numbers);

print("=== Geometry and Statistics Calculator ===");
print("Radius: " + radius);
print("Area: " + area);
print("Circumference: " + circumference);
print("Numbers: " + numbers);
print("Mean: " + mean);
print("Variance: " + variance);